<%= render partial: "static_pages/navbar" %>
<div class="container" style="padding-top: 50px;">
  <p id="notice"><%= notice %></p>
  <div class="col-lg-4" id="rooms">
    <h1>Chat Rooms</h1>
    <% @chat_rooms.each do |chat_room| %>
      <%= link_to "Join to #{chat_room.title}", chat_room, type: "button", class: "btn btn-primary" %><br>
      <hr>
    <% end %>
    <br>
    <%= link_to 'New Chat Room', new_chat_room_path %>
    </div>
</div>